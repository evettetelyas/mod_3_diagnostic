<center>
	<h1><%= @members.count %> Members</h1>

	<% @members.each do |member| %>
	<h2 class="name"><%= member.name %></h2>
	<p class="house"><i>House: <%= member.house %></i></p>
	<p class="role"><i>Role: <%= member.role %></i></p>
	<p class="patronus"><i>Patronus: <%= member.patronus %></i></p>
	<% end %>
</center>